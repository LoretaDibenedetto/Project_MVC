
@model List<Category>

<h1>Category List</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">Category Name</th>
            <th scope="col">Display Order</th>
           
       
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u => u.DisplayOrder))
        {
        <tr>
            <td>@obj.Name</td>
            <td>@obj.DisplayOrder</td>
          

       
          
        </tr>
        }
       
    </tbody>
</table>